for PKG in kde-i18n-af kde-i18n-ar kde-i18n-az kde-i18n-bg kde-i18n-bn kde-i18n-br kde-i18n-bs kde-i18n-ca kde-i18n-cs kde-i18n-cy kde-i18n-da kde-i18n-de kde-i18n-en_GB kde-i18n-el kde-i18n-eo kde-i18n-es kde-i18n-et kde-i18n-eu kde-i18n-fa kde-i18n-fi kde-i18n-fr kde-i18n-fy kde-i18n-ga kde-i18n-gl kde-i18n-he kde-i18n-hi kde-i18n-hr kde-i18n-hu kde-i18n-it kde-i18n-ja kde-i18n-km kde-i18n-kk kde-i18n-is kde-i18n-ko kde-i18n-lt kde-i18n-lv kde-i18n-mk kde-i18n-mn kde-i18n-ms kde-i18n-nb kde-i18n-nds kde-i18n-nl kde-i18n-nn kde-i18n-pa kde-i18n-pl kde-i18n-pt kde-i18n-pt_BR kde-i18n-ro kde-i18n-ru kde-i18n-rw kde-i18n-se kde-i18n-sk kde-i18n-sl kde-i18n-sr kde-i18n-sr@Latn kde-i18n-ss kde-i18n-sv kde-i18n-ta kde-i18n-tg kde-i18n-tr kde-i18n-uk kde-i18n-uz kde-i18n-vi kde-i18n-zh_CN kde-i18n-zh_TW ; do
  # Get locale name:
  KDELOCALE=$(echo $PKG | cut -f 3 -d -)
  if grep \"$PKG\" $TMP/SeTpkgs 1> /dev/null 2> /dev/null ; then
    # Add both the kde and koffice packages:
    echo "kde-i18n-$KDELOCALE: ADD" >> $TMP/SeTnewtag
    echo "koffice-l10n-$KDELOCALE: ADD" >> $TMP/SeTnewtag
  else
    # Skip both the kde and koffice packages:
    echo "kde-i18n-$KDELOCALE: SKP" >> $TMP/SeTnewtag
    echo "koffice-l10n-$KDELOCALE: SKP" >> $TMP/SeTnewtag
  fi
done
rm -f $TMP/SeTpkgs
