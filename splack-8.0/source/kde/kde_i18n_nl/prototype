VERSION=2.1.1
BUILD=1
ARCH=sparc
TAG=OPT

PKGNAME=kde_i18n_nl-$VERSION-$ARCH-$BUILD
MAINTAINER="David Cantrell <david@slackware.com>"

IGNOREPATH=/tmp:/proc:/dev:/root:/a:/var

PROGNAME="KDE Dutch Language Support"
DESC="\
kde-i18n-nl-$VERSION\n\
\n\
Dutch language support."

compile() {
   tar xvyf $CWD/kde-i18n-nl-$VERSION.tar.bz2
   cd kde-i18n-nl
   ./configure --prefix=/opt/kde
   make
}

install() {
   make install
}
