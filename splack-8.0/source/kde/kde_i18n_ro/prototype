VERSION=2.1.1
BUILD=1
ARCH=sparc
TAG=OPT

PKGNAME=kde_i18n_ro-$VERSION-$ARCH-$BUILD
MAINTAINER="David Cantrell <david@slackware.com>"

IGNOREPATH=/tmp:/proc:/dev:/root:/a:/var

PROGNAME="KDE Romanian Language Support"
DESC="\
kde-i18n-ro-$VERSION\n\
\n\
Romanian language support."

compile() {
   tar xvyf $CWD/kde-i18n-ro-$VERSION.tar.bz2
   cd kde-i18n-ro
   ./configure --prefix=/opt/kde
   make
}

install() {
   make install
}
