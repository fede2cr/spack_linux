VERSION=2.1.1
BUILD=1
ARCH=sparc
TAG=OPT

PKGNAME=kde_i18n_fi-$VERSION-$ARCH-$BUILD
MAINTAINER="David Cantrell <david@slackware.com>"

IGNOREPATH=/tmp:/proc:/dev:/root:/a:/var

PROGNAME="KDE Finnish Language Support"
DESC="\
kde-i18n-fi-$VERSION\n\
\n\
Finnish language support."

compile() {
   tar xvyf $CWD/kde-i18n-fi-$VERSION.tar.bz2
   cd kde-i18n-fi
   ./configure --prefix=/opt/kde
   make
}

install() {
   make install
}
