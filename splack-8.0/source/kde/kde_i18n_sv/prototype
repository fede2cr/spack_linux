VERSION=2.1.1
BUILD=1
ARCH=sparc
TAG=OPT

PKGNAME=kde_i18n_sv-$VERSION-$ARCH-$BUILD
MAINTAINER="David Cantrell <david@slackware.com>"

IGNOREPATH=/tmp:/proc:/dev:/root:/a:/var

PROGNAME="KDE Swedish Language Support"
DESC="\
kde-i18n-sv-$VERSION\n\
\n\
Swedish language support."

compile() {
   tar xvyf $CWD/kde-i18n-sv-$VERSION.tar.bz2
   cd kde-i18n-sv
   ./configure --prefix=/opt/kde
   make
}

install() {
   make install
}
