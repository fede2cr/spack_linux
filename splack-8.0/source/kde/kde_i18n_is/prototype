VERSION=2.1.1
BUILD=1
ARCH=sparc
TAG=OPT

PKGNAME=kde_i18n_is-$VERSION-$ARCH-$BUILD
MAINTAINER="David Cantrell <david@slackware.com>"

IGNOREPATH=/tmp:/proc:/dev:/root:/a:/var

PROGNAME="KDE Icelandic Language Support"
DESC="\
kde-i18n-is-$VERSION\n\
\n\
Icelandic language support."

compile() {
   tar xvyf $CWD/kde-i18n-is-$VERSION.tar.bz2
   cd kde-i18n-is
   ./configure --prefix=/opt/kde
   make
}

install() {
   make install
}
