VERSION=2.1.1
BUILD=1
ARCH=sparc
TAG=OPT

PKGNAME=kde_i18n_fr-$VERSION-$ARCH-$BUILD
MAINTAINER="David Cantrell <david@slackware.com>"

IGNOREPATH=/tmp:/proc:/dev:/root:/a:/var

PROGNAME="KDE French Language Support"
DESC="\
kde-i18n-fr-$VERSION\n\
\n\
French language support."

compile() {
   tar xvyf $CWD/kde-i18n-fr-$VERSION.tar.bz2
   cd kde-i18n-fr
   ./configure --prefix=/opt/kde
   make
}

install() {
   make install
}
