VERSION=2.1.1
BUILD=1
ARCH=sparc
TAG=OPT

PKGNAME=kde_i18n_no-$VERSION-$ARCH-$BUILD
MAINTAINER="David Cantrell <david@slackware.com>"

IGNOREPATH=/tmp:/proc:/dev:/root:/a:/var

PROGNAME="KDE Norwegian (Bokmaal) Language Support"
DESC="\
kde-i18n-no-$VERSION\n\
\n\
Norwegian (Bokmaal) language support."

compile() {
   tar xvyf $CWD/kde-i18n-no-$VERSION.tar.bz2
   cd kde-i18n-no
   ./configure --prefix=/opt/kde
   make
}

install() {
   make install
}
