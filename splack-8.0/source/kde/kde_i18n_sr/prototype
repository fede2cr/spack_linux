VERSION=2.1.1
BUILD=1
ARCH=sparc
TAG=OPT

PKGNAME=kde_i18n_sr-$VERSION-$ARCH-$BUILD
MAINTAINER="David Cantrell <david@slackware.com>"

IGNOREPATH=/tmp:/proc:/dev:/root:/a:/var

PROGNAME="KDE Serbian Language Support"
DESC="\
kde-i18n-sr-$VERSION\n\
\n\
Serbian language support."

compile() {
   tar xvyf $CWD/kde-i18n-sr-$VERSION.tar.bz2
   cd kde-i18n-sr
   ./configure --prefix=/opt/kde
   make
}

install() {
   make install
}
