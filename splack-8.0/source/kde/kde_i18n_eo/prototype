VERSION=2.1.1
BUILD=1
ARCH=sparc
TAG=OPT

PKGNAME=kde_i18n_eo-$VERSION-$ARCH-$BUILD
MAINTAINER="David Cantrell <david@slackware.com>"

IGNOREPATH=/tmp:/proc:/dev:/root:/a:/var

PROGNAME="KDE Esperanto Language Support"
DESC="\
kde-i18n-eo-$VERSION\n\
\n\
Esperanto language support."

compile() {
   tar xvyf $CWD/kde-i18n-eo-$VERSION.tar.bz2
   cd kde-i18n-eo
   ./configure --prefix=/opt/kde
   make
}

install() {
   make install
}
