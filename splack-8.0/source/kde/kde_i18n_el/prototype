VERSION=2.1.1
BUILD=1
ARCH=sparc
TAG=OPT

PKGNAME=kde_i18n_el-$VERSION-$ARCH-$BUILD
MAINTAINER="David Cantrell <david@slackware.com>"

IGNOREPATH=/tmp:/proc:/dev:/root:/a:/var

PROGNAME="KDE Greek Language Support"
DESC="\
kde-i18n-el-$VERSION\n\
\n\
Greek language support."

compile() {
   tar xvyf $CWD/kde-i18n-el-$VERSION.tar.bz2
   cd kde-i18n-el
   ./configure --prefix=/opt/kde
   make
}

install() {
   make install
}
