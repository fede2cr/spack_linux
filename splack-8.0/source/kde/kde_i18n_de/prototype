VERSION=2.1.1
BUILD=1
ARCH=sparc
TAG=OPT

PKGNAME=kde_i18n_de-$VERSION-$ARCH-$BUILD
MAINTAINER="David Cantrell <david@slackware.com>"

IGNOREPATH=/tmp:/proc:/dev:/root:/a:/var

PROGNAME="KDE German Language Support"
DESC="\
kde-i18n-de-$VERSION\n\
\n\
German language support."

compile() {
   tar xvyf $CWD/kde-i18n-de-$VERSION.tar.bz2
   cd kde-i18n-de
   ./configure --prefix=/opt/kde
   make
}

install() {
   make install
}
